[core]
api_key = <%= @_apikey %>
username = <%= @_webuser %>
password = <%= @_webpass %>
data_dir = <%= @_data %>
ssl_key = 
ssl_cert = 
permission_folder = 0755
development = 0
url_base = 
debug = 0
launch_browser = 0
port = 5050
permission_file = 0644
show_wizard = 0

[download_providers]

[updater]
notification = True
enabled = False
git_command = git
automatic = False

[automation]
rating = 7.0
votes = 1000
hour = 12
required_genres = 
year = 2011
ignored_genres = 

[manage]
library = <%= @_library %>
startup_scan = True
library_refresh_interval = 0
cleanup = True
enabled = 1

[renamer]
from = <%= @_from %>
to = <%= @_to  %>
file_name = <thename><cd>.<ext>
next_on_failed = True
default_file_action = move
ntfs_permission = False
check_space = True
nfo_name = <filename>.orig.<ext>
foldersep = 
cleanup = 1
unrar_modify_date = False
move_leftover = False
rename_nfo = True
folder_name = <namethe> (<year>)
run_every = 1
file_action = link
replace_doubles = True
unrar_path = 
enabled = 1
unrar = 1
separator = 
force_every = 2

[subtitle]
languages = 
force = False
enabled = False

[trailer]
quality = 720p
enabled = False
name = <filename>-trailer

<%- searchers = @_servers -%>
<%- if searchers -%>
  <%- searchers.keys.each do |searcher| -%>
[<%= searcher %>]
   <%- properties = searchers[searcher] -%>
   <%- properties.keys.each do |name| -%>
     <%= name -%> = <%= properties[name] -%>
   <%- end -%>
  <%- end -%>
<%- end -%>

[notification_providers]

<%- provids = @_notification_providers -%>
<%- if provids -%>
