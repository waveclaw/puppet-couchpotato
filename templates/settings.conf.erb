<%- searchers = scope.function_hiera(['couchpotato::searchers']) -%>
<%- if searchers -%>
  <%- searchers.keys.each do |searcher| -%>
[<%= searcher %>]
   <%- properties = searchers[searcher] -%>
   <%- properties.keys.each do |name| -%>
     <%= name -%> = <%= properties[name] -%>
   <%- end -%>
  <%- end -%>
<%- end -%>

